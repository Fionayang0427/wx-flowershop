<!--pages/order/order.wxml-->

<view>总消费金额为{{allprice}}元</view>
<!-- 获取商品 -->
<view class="my-goods" >
	<view wx:for="{{goods}}">
		<view class="goodbox">
    <view>下单时间{{item.otime}}</view>
			<image src="{{item.img}}"></image>
			<view class="hidden">{{item.title}}</view>
			<view >{{item.allprice}}元</view>
      <text>{{item.gcount}}个</text>
		</view>
	</view>
</view>

